<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:c="http://www.springframework.org/schema/c"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="someString" class="java.lang.String">
		<constructor-arg value="test"></constructor-arg>
	</bean>
	
	<bean id="someBean"
		class="pl.mrmario.tutorials.spring.owczarek.cz8.SomeBean">
		<property name="stringValue" value="Cosik"></property>
		<property name="intValue" value="44"></property>
	</bean>
</beans>
